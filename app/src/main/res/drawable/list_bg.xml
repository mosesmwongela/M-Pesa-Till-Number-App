<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#E9967A"/>
            <stroke
                android:width="1dp"
                android:color="#E9967A"
                />
            <corners
                android:radius="10dp"/>

        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#FFFFFF"/>
            <stroke
                android:width="1dp"
                android:color="#FFFFFF"
                />
            <corners
                android:radius="10dp"
                />

        </shape>
    </item>
</selector>